{
    "activationEvents": [
        "onStartupFinished"
    ],
    "categories": [
        "Debuggers",
        "Formatters"
    ],
    "contributes": {
        "configuration": {
            "properties": {}
        },
        "commands": [
            {
                "command": "ConsoleFilter.filterMessages",
                "title": "Filter Debug Console Messages"
            }
        ]
    },
    "description": "A filter for the Debug Console.",
    "devDependencies": {
        "@types/jest": "^29.5.13",
        "@types/mocha": "^10.0.10",
        "@types/node": "22.x",
        "@types/vscode": "^1.93.0",
        "@typescript-eslint/eslint-plugin": "^8.30.1",
        "@typescript-eslint/parser": "^8.30.1",
        "@vscode/test-cli": "^0.0.10",
        "@vscode/test-electron": "^2.5.2",
        "eslint": "^9.24.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.3.2",
        "typescript": "^5.8.3"
    },
    "directories":
    {
        "test": "tests"
    },
    "displayName": "ConsoleFilter",
    "engines": {
        "vscode": "^1.93.0"
    },
    "icon": "images/ConsoleFilter512.png",
    "main": "./out/src/Extension.js",
    "name": "ConsoleFilter",
    "publisher": "eggnstone",
    "repository": "https://github.com/eggnstone/ConsoleFilterVSCodePlugin",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "lint": "eslint src --ext ts",
        "test": "jest --config jestconfig.json"
    },
    "version": "1.0.2"
}
